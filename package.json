{
  "name": "react-prpl-boilerplate",
  "version": "0.1.0",
  "description": "A simple boilerplate for learn PRPL pattern with React.",
  "scripts": {
    "start": "set NODE_ENV=development node && node_modules/.bin/webpack-dev-server --config ./config/dev.config.js",
    "build": "set NODE_ENV=production node && node_modules/.bin/webpack --config ./config/prod.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.2",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-loadable": "^5.5.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  }
}
